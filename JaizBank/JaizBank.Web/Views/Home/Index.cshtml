@model List<TransactionsViewModel>


@{
    ViewData["Title"] = "Home Page";
}


@if(Model.Any())
{

    @foreach(var items in Model)
    {
       
        <div class="wrapper">

        <div class="merchant">
            <h3>**** <span>MERCHANT COPY</span> ****</h3>
            <ul>
              <li>MERCHANT NAME : </li>
                <li> @items.MerchantName</li>
              <li>LOCATION : </li>
                <li>  @items.Location</li>
              <li>URT</li>
              <li>TERMINAL ID : @items.TerminalId</li>
            </ul>
              
        </div>
        <div class="purchase">
            <h1>PURCHASE</h1>
           
            <ul>
              <li>STAN: @items.Stan </li>
              <li>DATE/TIME: @items.DateTime</li>
              <li class="dot">---------------------------------------</li>
              <li>AMOUNT: NGN @items.Amount</li>
              <li class="dot">----------------------------------------</li>

            </ul>
        </div>
        <div id="amount">
           <ul>
               
                <li> Debit MasterCard</li>
                 <li>@items.DebitCardNo</li>
              <li>369/GLOBAL ACCELEREX</li>

              <li>EXPIRY DATE: @items.ExpireDate</li>
              <li>AUTHORIZATION CODE: @items.AuthorizationCode</li>
              <li> OFFLINE CVM</li>
              <li class="dot">-----------------------------------------</li>
           </ul>
        </div>
        <div class="mastercard">
            <h1>TRANSACTION DECLINED</h1>
            <ul>
                <li>RESPONSE CODE: @items.ResponseCode</li>
                <li>Issuer or switch inoperative</li>
                <li>AID: @items.AID</li>
                <li>RRN: @items.RRN</li>
                <li>Accelerex @items.Accelerex</li>
                <li>PTAD: @items.PTAD</li>
                <li class="dot">-----------------------------------------</li>
            </ul>
        </div>
       
        </div>
      

  
    }
}